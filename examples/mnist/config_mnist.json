{
  "dataset": {
    "loader": {
      "import": "KeySelector",
      "params": {
        "inputs": {
          "img": "x"
        },
        "outputs": {
          "target": "y"
        }
      }
    }
  },
  "model": {
    "network": {
      "import": "example.net"
    },
    "outputs": [{
      "name": "target",
      "loss": {
        "import": "sparse_categorical_crossentropy"
      },
      "metrics": [{
        "import": "accuracy"
      }]
    }]
  },
  "solver": {
    "optimizer": {
      "import": "Adam",
      "learning_rate": 1e-4
    },
    "epochs": 5,
    "batch_size": 128
  }
}
